# %% VM RAMP

###### settings
ramp_on = 'P1'; step_on = 'P2' # 'P1' | 'P2'

valueRamp, deltaRamp, timeRamp = 1.102, 0.015, 0.010
valueStep, deltaStep, nbStep = 0.602, 0.008, 101

sync_time_after_ramp = 0.01
