def get(nb=1):
    nb_pulse=ats.nbwindows.get()
    acq = None
    awg.run(True)
    
    for _ in range(nb):
        data = ats.fetch_all.get()
        data = data[2].reshape((nb_pulse,-1))
        if acq is None:
            acq = data
        else: 
            acq = np.concatenate((acq, data))
    
    awg.run(False)
    return acq


